<?xml version="1.0"?>
<!DOCTYPE assetlist>

<assetlist author="Areldir" name="Mess_Hall">
<!-- Note: Mess Hall Variants for Central, North and South Climates Respectively -->
    <image id="Mess OFF Central" src="Mess_Hall_Central" />
    <image id="Mess OFF North" src="Mess_Hall_North" />	
    <image id="Mess OFF South" src="Mess_Hall_South" />	

<!-- Note: Mess Hall ON Animations for Central, North and South Climates Respectively -->
    <image id="Mess ON Central" >
		<layer group="this" image="Mess OFF Central" />
        <animation speed="6" frames="3" reversible="true" />
    </image>
    <image src="Mess_Hall_Central_Anim_01" />
    <image src="Mess_Hall_Central_Anim_02" />
    <image src="Mess_Hall_Central_Anim_03" />


    <image id="Mess ON North" >
		<layer group="this" image="Mess OFF North" />
        <animation speed="6" frames="3" reversible="true" />
    </image>
    <image src="Mess_Hall_North_Anim_01" />
    <image src="Mess_Hall_North_Anim_02" />
    <image src="Mess_Hall_North_Anim_03" />


    <image id="Mess ON South" >
		<layer group="this" image="Mess OFF South" />
        <animation speed="6" frames="3" reversible="true" />
    </image>
    <image src="Mess_Hall_South_Anim_01" />
    <image src="Mess_Hall_South_Anim_02" />
    <image src="Mess_Hall_South_Anim_03" />



</assetlist>
