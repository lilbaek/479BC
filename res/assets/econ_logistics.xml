<?xml version="1.0"?>
<!DOCTYPE assetlist>

<!-- Unless specifically mentioned, all assets created by Areldir -->
<assetlist name="Econ_Logistics">
	<!-- Note: Caravanserai OFF Variant for Northern/Central and Southern Climates Respectively -->
	<image id="Caravanserai N OFF" src="Caravanserai_n_OFF"/>
	<image id="Caravanserai S OFF" src="Caravanserai_s_OFF"/>
	<!-- Note: Caravanserai ON Variant for Northern/Central Climate -->
	<image id="Caravanserai N ON" width="238" height="155">
		<layer group="this" image="Caravanserai N OFF"/>
		<animation speed="8" x="44" y="38" reversible="true">
			<frame src="Caravanserai_n_ON_01"/>
			<frame src="Caravanserai_n_ON_02"/>
			<frame src="Caravanserai_n_ON_03"/>
		</animation>
	</image>
	<!-- Note: Caravanserai ON Variant for Southern Climate -->
	<image id="Caravanserai S ON" width="238" height="148">
		<layer group="this" image="Caravanserai S OFF"/>
		<animation speed="8" x="44" y="30" reversible="true">
			<frame src="Caravanserai_s_ON_01"/>
			<frame src="Caravanserai_s_ON_02"/>
			<frame src="Caravanserai_s_ON_03"/>
		</animation>
	</image>
	<!-- Note: Caravanserai Construction 0% Phase -->
	<image id="Caravanserai Cons" src="Caravanserai_Construction_0%"/>
	<!-- Note: Cart Depot OFF Variants for Northern, Central and Southern Climates Respectively -->
	<image id="Cart Depot N OFF" src="Cart_Depot_OFF_N"/>
	<image id="Cart Depot C OFF" src="Cart_Depot_OFF_C"/>
	<image id="Cart Depot S OFF" src="Cart_Depot_OFF_S"/>
	<!-- Note: Cart Depot ON Variants for Northern, Central and Southern Climates Respectively -->
	<image id="Cart Depot N ON">
		<layer group="this" image="Cart Depot N OFF"/>
		<animation speed="7" x="47" y="31" reversible="true">
			<frame src="Cart_Depot_ON_01"/>
			<frame src="Cart_Depot_ON_02"/>
			<frame src="Cart_Depot_ON_03"/>
		</animation>
	</image>
	<image id="Cart Depot C ON">
		<layer group="this" image="Cart Depot C OFF"/>
		<animation speed="7" x="47" y="31" reversible="true">
			<frame src="Cart_Depot_ON_01"/>
			<frame src="Cart_Depot_ON_02"/>
			<frame src="Cart_Depot_ON_03"/>
		</animation>
	</image>
	<image id="Cart Depot S ON">
		<layer group="this" image="Cart Depot S OFF"/>
		<animation speed="7" x="47" y="31" reversible="true">
			<frame src="Cart_Depot_ON_01"/>
			<frame src="Cart_Depot_ON_02"/>
			<frame src="Cart_Depot_ON_03"/>
		</animation>
	</image>
	<!-- Note: Lighthouse Construction 01-04 are the 0%, 25%, 50%, and 75% construction phases respectively -->
	<image id="Lighthouse Const 01" src="Lighthouse_Construction_01"/>
	<image id="Lighthouse Const 02" src="Lighthouse_Construction_02"/>
	<image id="Lighthouse Const 03" src="Lighthouse_Construction_03"/>
	<image id="Lighthouse Const 04" src="Lighthouse_Construction_04"/>
	<!-- Note: Lighthouse OFF Variant -->
	<image id="Lighthouse OFF" src="Lighthouse_OFF"/>
	<!-- Note: Lighthouse ON Variant -->
	<image id="Lighthouse ON">
		<layer group="this" image="Lighthouse OFF"/>
		<animation speed="5" x="74" y="1" reversible="false">
			<frame src="Lighthouse_ON_01"/>
			<frame src="Lighthouse_ON_02"/>
			<frame src="Lighthouse_ON_03"/>
			<frame src="Lighthouse_ON_04"/>
			<frame src="Lighthouse_ON_05"/>
		</animation>
	</image>
</assetlist>
	