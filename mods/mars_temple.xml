    <image id="Mars Complex Const 02" src="Mars_Temple_Complex_Construction_02" />	
    <image id="Mars Complex Const 03" src="Mars_Temple_Complex_Construction_03" />	
    <image id="Mars Complex Const 04" src="Mars_Temple_Complex_Construction_04" />	
