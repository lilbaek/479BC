	    <layer src="Mars_Temple_Complex_Construction_05" />
		<layer group="74" image="1" x="90" y="8" />
	</image>
<!-- Note: Mars Complex ON/OFF States -->
    <image id="Mars Complex Off" width="418" height="231">
        <layer src="Mars_Temple_Complex_Base_Layer" />
